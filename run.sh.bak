#!/bin/bash

# 检查Python虚拟环境是否存在
if [ -d "venv" ]; then
    echo "Activating virtual environment (venv)..."
    #注意虚拟环境名：venv
    source venv/bin/activate
else
    echo "Virtual environment (venv) not found. Activate it manually if needed."
fi

# 执行Python项目(注意：默认启动程序为app.py，请替换实际的项目名称)
python app.py
